        rdfs:subPropertyOf :hasCharacteristic ;
        rdfs:domain :PersonneCybersecurite ;
        rdfs:range [ rdf:type owl:Restriction ;
                     owl:onProperty owl:topDataProperty ;
                     owl:someValuesFrom xsd:dateTime
                   ] .


